<h1>Artworks matching "<%= params[:query] %>"</h1>

<table>
  <tr>
    <th>Object</th>
    <th>Creator</th>
    <th>Title</th>
    <th>Description</th>
    <th>Location</th>
    <th>Recfrom</th>
    <th>Date</th>
    <th>Filename</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @artworks.each do |artwork| %>
  <tr>
    <td><%= artwork.object %></td>
    <td><%= artwork.creator %></td>
    <td><%= artwork.title %></td>
    <td><%= artwork.description %></td>
    <td><%= artwork.location %></td>
    <td><%= artwork.recfrom %></td>
    <td><%= artwork.date %></td>
    <td><%= artwork.filename %></td>
  </tr>
<% end %>
</table>

<br />
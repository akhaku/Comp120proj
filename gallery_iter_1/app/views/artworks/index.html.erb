<h1>Listing artworks</h1>

<table>
  <tr>
    <th>Object</th>
    <th>Creator</th>
    <th>Title</th>
    <th>Description</th>
    <th>Location</th>
    <th>Recfrom</th>
    <th>Date</th>
    <th>Filename</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @artworks.each do |artwork| %>
  <tr>
    <td><%= artwork.object %></td>
    <td><%= artwork.creator %></td>
    <td><%= artwork.title %></td>
    <td><%= artwork.description %></td>
    <td><%= artwork.location %></td>
    <td><%= artwork.recfrom %></td>
    <td><%= artwork.date %></td>
    <td><%= artwork.filename %></td>
    <td><%= link_to 'Show', artwork %></td>
    <td><%= link_to 'Edit', edit_artwork_path(artwork) %></td>
    <td><%= link_to 'Destroy', artwork, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Artwork', new_artwork_path %>
